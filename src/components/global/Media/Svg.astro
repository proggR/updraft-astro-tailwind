--- 
import { Attributes } from "../Style";
const props = Astro.props 
const atts = Attributes(props)
const x=props.x?props.x:32, y=props.y?props.y:32 //assumes socials? maybe weird defaults
--- 

<svg xmlns="http://www.w3.org/2000/svg" viewBox={`0 0 ${x} ${y}`} {...atts}>
    <path d={props.data}></path>
</svg>
