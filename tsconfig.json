{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": ["astro/client"],
		"baseUrl": ".",
		"paths": {
			"@globals": ["src/components/global"],
			"@component/*": ["src/components/*"],
			"@style/*": ["src/styles/*"],
			"@globalStructure/*": ["src/styles/components/global/structure/*"],
			"@globalStyle/*": ["src/styles/components/global/style/*"],
			"@affordableStructure/*": ["src/styles/components/affordable/structure/*"],
			"@affordableStyle/*": ["src/styles/components/affordable/style/*"],
			"@util/*": ["utils/*"],
			"@img/*": ["src/assets/img/*"],
			"@svg/*": ["src/assets/svg/*"],
			"@icon/*": ["src/assets/svg/icon/*"]
		}
	}
}
